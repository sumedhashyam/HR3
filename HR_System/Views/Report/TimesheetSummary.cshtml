@{
    ViewBag.Title = "TimesheetSummary";
    Layout = "~/Views/Shared/Main_Layout.cshtml";
}

@{
    Guid EmployeeID = new Guid(ViewBag.EmployeeID.ToString());
    Guid TimeSheetMasterID = new Guid(ViewBag.TimeSheetMasterID.ToString());
    string returnUrl = ViewBag.returnUrl != null ? ViewBag.returnUrl.ToString() : null;
}

<script type="text/javascript">
    $(function () {
        reports.loadTimesheetSummary('@EmployeeID', '@TimeSheetMasterID','@returnUrl')
    });
</script>
@{Html.RenderPartial("Notification");}
<div id="main-content" class="row-fluid">
</div>


